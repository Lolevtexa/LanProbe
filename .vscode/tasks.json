{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [ "build", "${workspaceFolder}/LanProbe.csproj", "/property:GenerateFullPaths=true", "/consoleloggerparameters:NoSummary" ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "process",
      "args": [ "run", "--project", "${workspaceFolder}/LanProbe.csproj" ]
    }
  ]
}
